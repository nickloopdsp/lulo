# OpenAI API configuration (required for AI image analysis)
OPENAI_API_KEY=your_openai_api_key_here

# Google Vision API (optional - for enhanced image recognition)
GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Database configuration (optional - defaults to local SQLite)
DATABASE_URL=sqlite:lulo.db

# Server configuration (optional - defaults to port 5000)
PORT=5001

# Session secret for authentication
SESSION_SECRET=your_session_secret_here

# Node environment
NODE_ENV=development
